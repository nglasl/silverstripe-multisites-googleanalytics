{
	"name": "symbiote/silverstripe-multisites-googleanalytics",
	"description": "Adds Google Analytics tracking code to each multisite",
	"type": "silverstripe-vendormodule",
	"keywords": ["silverstripe", "multisites", "analytics"],
	"require":
	{
		"silverstripe/framework": "~4.0",
		"symbiote/silverstripe-multisites": "~5.0"
	},
	"autoload": {
		"psr-4": {
			"Symbiote\\Multisites\\GoogleAnalytics\\": "src/",
			"Symbiote\\Multisites\\GoogleAnalytics\\Tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0.x-dev"
		},
		"expose": [
			"client"
		]
	},
	"minimum-stability": "dev"
}
